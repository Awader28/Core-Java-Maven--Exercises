public class IgnoreAnnot {

	@Test

	void demo () {		
		Operation op=new Operation();

		int x=90;
		int z= op.mul(45,2);
		System.out.println("HELLO");
		assertEquals(x,z)
	}
	
	@Test
	void display() {
		Operation op=new Operation();
