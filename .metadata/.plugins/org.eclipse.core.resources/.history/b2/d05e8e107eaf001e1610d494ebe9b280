package com.internship.day15.junit;

import static org.junit.jupiter.api.Assertions.assertEquals;

import org.junit.jupiter.api.Test;

public class OpTest {
	@Test	
	public void display() {
		Operation op=new Operation();
		int x=50;
		int z=op.add(20, 20);
		assertEquals(x,z);
	}
	@Test	
	public void displaySub() {
		Operation op=new Operation();
		int x=50;
		int z=op.add(20, 20);
		assertEquals(x,z);
	}
	@Test	
	public void displayMul() {
		Operation op=new Operation();
		int x=50;
		int z=op.add(20, 20);
		assertEquals(x,z);
	}
	@Test	
	public void displayDiv() {
		Operation op=new Operation();
		int x=50;
		int z=op.add(20, 20);
		assertEquals(x,z);
	}

}
